    "def match_words(words):\n",
    "  ctr = 0\n",
    "  for word in words:\n",
    "    if len(word) > 1 and word[0] == word[-1]:\n",
    "      ctr += 1\n",
    "  return ctr\n",
    "print(match_words([ 'abc', 'ef','13311','abcd','121']))"
   ]
